  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@ianwremmel/eslint-plugin-ianwremmel": "^2.0.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "autoprefixer": "^9.2.1",
    "condition-circle": "^1.6.0",
    "cssnano": "^4.1.4",
    "eslint": "^5.7.0",
