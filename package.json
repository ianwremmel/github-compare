    "eslint-plugin-vue": "^4.7.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "markdown-toc": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "postcss-import": "^12.0.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.17",
    "vue-template-compiler": "^2.5.17"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "bin/**": "npm run eslint -- "
